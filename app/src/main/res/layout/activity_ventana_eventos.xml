<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#292929"
    tools:context=".eventos.ventanaEventos">

    <TextView
        android:id="@+id/textView9"
        android:layout_width="320dp"
        android:layout_height="36dp"
        android:fontFamily="@font/departuremonoregular"
        android:gravity="center"
        android:text="Eventos"
        android:textColor="#FFFFFF"
        android:textColorLink="#000000"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.056" />

    <ImageView
        android:id="@+id/evento3"
        android:layout_width="363dp"
        android:layout_height="153dp"
        android:layout_gravity="center"
        android:clickable="true"
        android:contentDescription="Icono"
        android:onClick="onEvento"
        android:fadingEdge="none"
        android:focusable="true"
        android:requiresFadingEdge="none"
        android:src="@drawable/eventos_signo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.863" />

    <ImageView
        android:id="@+id/evento2"
        android:layout_width="363dp"
        android:layout_height="153dp"
        android:layout_gravity="center"
        android:clickable="true"
        android:contentDescription="Icono"
        android:onClick="onEvento"
        android:fadingEdge="none"
        android:focusable="true"
        android:requiresFadingEdge="none"
        android:src="@drawable/eventos_summerd"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.536" />

    <ImageView
        android:id="@+id/evento1"
        android:layout_width="363dp"
        android:layout_height="153dp"
        android:layout_gravity="center"
        android:clickable="true"
        android:contentDescription="Icono"
        android:onClick="onEvento"
        android:fadingEdge="none"
        android:focusable="true"
        android:requiresFadingEdge="none"
        android:src="@drawable/eventos_origen"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.23" />


</androidx.constraintlayout.widget.ConstraintLayout>